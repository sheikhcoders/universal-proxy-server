{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "universal-proxy-server",
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node@4.x"
    }
  ],
  "routes": [
    {
      "src": "/v1/chat/completions",
      "methods": ["POST"],
      "dest": "/dist/index.js"
    },
    {
      "src": "/v1/messages",
      "methods": ["POST"],
      "dest": "/dist/index.js"
    },
    {
      "src": "/health",
      "methods": ["GET"],
      "dest": "/dist/index.js"
    }
  ],
  "regions": ["iad1"]
}
