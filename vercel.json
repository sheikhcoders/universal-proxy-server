{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "name": "universal-proxy-server",
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "regions": ["iad1"]
}
